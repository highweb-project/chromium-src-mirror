interface ApplicationInfo {
	readonly attribute DOMString name;
	readonly attribute DOMString className;
	readonly attribute DOMString dataDir;
	readonly attribute boolean enabled;
	readonly attribute long flags;
	readonly attribute DOMString permission;
	readonly attribute DOMString processName;
	readonly attribute long targetSdkVersion;
	readonly attribute long theme;
	readonly attribute long uid;
	readonly attribute DOMString packageName;
};
